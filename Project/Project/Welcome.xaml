    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
   d:DesignHeight="300" d:DesignWidth="400" Height="480" Width="640" >

    <Canvas x:Name="LayoutRoot" Background="#FFE6FFFF">
        <Image   x:Name="bg"  Canvas.Left="0"  Source="/background.png" />
        <Button Content="Start" Canvas.Left="290" Canvas.Top="208" Width="101" Height="39" Click="Button_Click"/>
        <Button Content="Check Score" Canvas.Left="290" Canvas.Top="279" Width="101" Height="39"/>
    </Canvas>
</UserControl>
